{
	"name": "DF_Load_DimRegion",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "sf_RegionCodesStg",
						"type": "DatasetReference"
					},
					"name": "sourceRegion"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DimRegionSF",
						"type": "DatasetReference"
					},
					"name": "sinkDimRegion"
				}
			],
			"transformations": [
				{
					"name": "select1"
				},
				{
					"name": "surrogateRegionIDSK"
				}
			],
			"scriptLines": [
				"source(output(",
				"          COUNTRY_CODE as string,",
				"          COUNTRY_NAME as string,",
				"          DI_JOB_ID as decimal(38,0),",
				"          DI_LOAD_DT as date",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'table') ~> sourceRegion",
				"sourceRegion select(mapColumn(",
				"          COUNTRY_CODE,",
				"          COUNTRY_NAME,",
				"          DI_JOB_ID,",
				"          DI_LOAD_DT",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 keyGenerate(output(Region_ID_SK as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateRegionIDSK",
				"surrogateRegionIDSK sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     truncate:true,",
				"     format: 'table',",
				"     stageInsert: true,",
				"     mapColumn(",
				"          COUNTRY_CODE,",
				"          COUNTRY_NAME,",
				"          DI_JOB_ID,",
				"          DI_LOAD_DT,",
				"          Region_ID_SK",
				"     )) ~> sinkDimRegion"
			]
		}
	}
}