{
	"name": "df_TitleCrew",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_TitleCrewStg",
						"type": "DatasetReference"
					},
					"name": "dfTitleCrewStg"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "snowflake_TitleCrewStg",
						"type": "DatasetReference"
					},
					"name": "snowflakeTitleCrewStg"
				}
			],
			"transformations": [
				{
					"name": "derivedColumnDirectorsWriters"
				},
				{
					"name": "flattenDirectors"
				},
				{
					"name": "flattenWriters"
				}
			],
			"scriptLines": [
				"source(output(",
				"          tconst as string,",
				"          directors as string,",
				"          writers as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> dfTitleCrewStg",
				"dfTitleCrewStg derive(directors = split(directors,','),",
				"          writers = split(writers,',')) ~> derivedColumnDirectorsWriters",
				"derivedColumnDirectorsWriters foldDown(unroll(directors),",
				"     mapColumn(",
				"          tconst,",
				"          directors,",
				"          writers",
				"     ),",
				"     skipDuplicateMapInputs: false,",
				"     skipDuplicateMapOutputs: false) ~> flattenDirectors",
				"flattenDirectors foldDown(unroll(writers),",
				"     mapColumn(",
				"          tconst,",
				"          directors,",
				"          writers",
				"     ),",
				"     skipDuplicateMapInputs: false,",
				"     skipDuplicateMapOutputs: false) ~> flattenWriters",
				"flattenWriters sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          TCONST as string,",
				"          DIRECTORS as string,",
				"          WRITERS as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     stageInsert: true,",
				"     mapColumn(",
				"          TCONST = tconst,",
				"          DIRECTORS = directors,",
				"          WRITERS = writers",
				"     )) ~> snowflakeTitleCrewStg"
			]
		}
	}
}